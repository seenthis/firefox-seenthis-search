{
  "description": "This extension is search engine for seenthis.net allowing to find your posts very quickly.",
  "manifest_version": 2,
  "name": "seenthis search",
  "version": "0.1.0",
  "homepage_url": "https://github.com/seenthis/firefox-seenthis-search",
  "icons": {
    "48": "icons/seenthis-48.png"
  },
  "applications": {
    "gecko": {
      "id": "seenthis_search@seenthis.net",
      "strict_min_version": "45.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/seenthis.svg"
  },
  "commands": {
    "openview": {
      "suggested_key": { "default": "Ctrl+Shift+B" },
      "description": "Open seenthis search"
    }
  }
}
